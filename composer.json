{
    "name": "chubbyphp/chubbyphp-validation",
    "description": "Chubbyphp Validate",
    "keywords": ["chubbyphp", "slim", "validation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/log": "^1.0"
    },
    "conflict": {
        "symfony/validator": "<3.4,>=5.0"
    },
    "require-dev": {
        "chubbyphp/chubbyphp-mock": "^1.4",
        "doctrine/common": "^2.2",
        "friendsofphp/php-cs-fixer": "^2.15.3",
        "infection/infection": "^0.14.1",
        "nunomaduro/phpinsights": "^1.9",
        "phploc/phploc": "^5.0",
        "phpstan/phpstan": "^0.11.16",
        "phpunit/phpunit": "^8.4",
        "pimple/pimple": "^3.2.3",
        "psr/container": "^1.0",
        "symfony/validator": "^3.4|^4.2"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\Validation\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Chubbyphp\\Tests\\Validation\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    }
}
